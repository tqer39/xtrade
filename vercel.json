{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "feature/*": false,
      "fix/*": false,
      "hotfix/*": false,
      "renovate/*": false
    }
  },
  "crons": [
    {
      "path": "/api/cron/trust-worker",
      "schedule": "*/5 * * * *"
    }
  ]
}
