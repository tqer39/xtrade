{
  "name": "xtrade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check ./app ./src",
    "lint:fix": "biome check --write ./app ./src",
    "format": "biome format --write ./app ./src",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx src/db/seed/index.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "scrape:cards": "npx tsx scripts/scraper/index.ts",
    "scrape:cards:dry-run": "npx tsx scripts/scraper/index.ts --dry-run"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@neondatabase/serverless": "1.0.2",
    "@radix-ui/react-alert-dialog": "1.1.15",
