    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/pg": "8.16.0",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.15",
    "drizzle-kit": "^0.31.8",
    "jsdom": "27.3.0",
    "sharp": "^0.34.5",
    "textlint": "15.5.0",
    "textlint-filter-rule-comments": "1.2.2",
