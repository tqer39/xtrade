# xtrade dev/vercel 環境用の環境変数
# このファイルをコピーして .envrc を作成してください
# cp .envrc.example .envrc

# Vercel API Token (必須)
export VERCEL_API_TOKEN="your-vercel-api-token-here"

# AWS Vault Profile (Terraform state 用)
export AWS_VAULT_PROFILE="xtrade-dev"

# BetterAuth 認証設定 (必須)
export TF_VAR_better_auth_secret="your-better-auth-secret-here"
export TF_VAR_twitter_client_id="your-twitter-client-id-here"
export TF_VAR_twitter_client_secret="your-twitter-client-secret-here"
